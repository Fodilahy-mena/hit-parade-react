{"line":86,"column":8}},{"generated":{"line":144,"column":0},"source":"Context.js","original":{"line":78,"column":36}},{"generated":{"line":145,"column":0},"source":"Context.js","original":{"line":78,"column":11}},{"name":"props","generated":{"line":145,"column":5},"source":"Context.js","original":{"line":87,"column":17}},{"generated":{"line":145,"column":10},"source":"Context.js","original":{"line":87,"column":22}},{"name":"children","generated":{"line":145,"column":11},"source":"Context.js","original":{"line":87,"column":23}},{"generated":{"line":145,"column":19},"source":"Context.js","original":{"line":78,"column":11}},{"generated":{"line":145,"column":20},"source":"Context.js","original":{"line":78,"column":4}},{"generated":{"line":146,"column":0},"source":"Context.js","original":{"line":89,"column":1}}],"sources":{"Context.js":"import React, {useState, useEffect} from 'react'\r\nimport songsData from './songsData.json';\r\n\r\nconst Context = React.createContext();\r\nfunction ContextProvider(props) {\r\n    const [allSongs, setAllSongs] = useState([]);\r\n    const [cartItems, setCartItems] = useState([]);\r\n    \r\n    useEffect(() => {\r\n        setAllSongs(songsData);\r\n    }, []);\r\n\r\n    // updating isFavorite when clicked\r\n    function toggleFavorite(id) {\r\n        const newSongsArray = allSongs.map(song => {\r\n            // if it is the one, let's return an updated object\r\n            if(song.id === Number(id)) {\r\n                \r\n                // update this element\r\n                return {\r\n                    ...song,\r\n                    isFavorite: !song.isFavorite,}\r\n                    \r\n            }\r\n            // it's not the one I am looking for, therefore, I will not change it\r\n            return song;\r\n        })\r\n        setAllSongs(newSongsArray)\r\n        console.log(\"updated nw sng\",allSongs)\r\n    }\r\n    function toggleUpVotes(id) {\r\n        const songsArrayUpdateUpVotes = allSongs.map(song => {\r\n            // if it is the one, let's return an updated object\r\n            if(song.id === Number(id)) {\r\n                \r\n                // update this element\r\n                return {\r\n                    ...song,\r\n                    upVotes: song.upVotes + 1,}\r\n                    \r\n            }\r\n            // it's not the one I am looking for, therefore, I will not change it\r\n            return song;\r\n        })\r\n        setAllSongs(songsArrayUpdateUpVotes)\r\n    }\r\n    function toggleDownVotes(id) {\r\n        const songsArrayUpdateDownVotes = allSongs.map(song => {\r\n            // if it is the one, let's return an updated object\r\n            if(song.id === Number(id)) {\r\n                \r\n                // update this element\r\n                return {\r\n                    ...song,\r\n                    downVotes: song.downVotes + 1,}\r\n                    \r\n            }\r\n            // it's not the one I am looking for, therefore, I will not change it\r\n            return song;\r\n        })\r\n        setAllSongs(songsArrayUpdateDownVotes)\r\n    }\r\n    function addSongToCart(song) {\r\n        // how to add an element to an array, in an immutable way\r\n        // push is mutable (array.push(newstaff))\r\n        // map is immutable (let newArray = array.map())\r\n        \r\n        setCartItems(prevItem => [...prevItem, song]);\r\n    }\r\n\r\n    function removeSongFromCart(song) {\r\n        setCartItems(prevItems => prevItems.filter(item => item.id !== song));\r\n        }\r\n\r\n    function boughtCart() {\r\n        setCartItems([]);\r\n    }\r\n    return <Context.Provider value={{\r\n        allSongs,\r\n        toggleFavorite,\r\n        toggleUpVotes, \r\n        toggleDownVotes, \r\n        addSongToCart, \r\n        cartItems,\r\n        removeSongFromCart,\r\n        boughtCart}}>\r\n                {props.children}\r\n            </Context.Provider>\r\n}\r\n\r\nexport { ContextProvider, Context};"},"lineCount":null}},"error":null,"hash":"577e405d196bf3f62d6f947a61a22b60","cacheData":{"env":{}}}